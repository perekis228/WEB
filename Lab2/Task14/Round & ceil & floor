<?php
$num1 = sqrt(379);
$num1Dot0 = round($num1, $precision = 0, $mode = PHP_ROUND_HALF_UP);
$num1Dot1 = round($num1, $precision = 1, $mode = PHP_ROUND_HALF_UP);
$num1Dot2 = round($num1, $precision = 2, $mode = PHP_ROUND_HALF_UP);

$num2 = sqrt(587);
$num2Ceil = ceil($num2);
$num2Floor = floor($num2);
$num2mas = ["ceil"=>$num2Ceil, "floor"=>$num2Floor];
?>
