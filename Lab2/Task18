<?php
function ten($num1, $num2){
    return 10 < $num1 + $num2;
}

function equal($num1, $num2){
    return $num1 === $num2;
}

$test = 0;
if ($test == 0) echo 'верно'."\n";

$age = 29;
if(10 > $age or 99 < $age){
    echo "Число меньше 10 или больше 99";
}
else
{
    $sum = $age % 10 + ($age / 10) % 10;
    if(9 => $sum): echo "Сумма цифр однозначна";
    else: echo "Сумма цифр двузначна";
    endif;
}

$arr = [1, 2, 3];
if(count($arr) === 3) echo array_sum($arr);
?>
